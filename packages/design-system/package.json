{
  "name": "design-system",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist",
    "./styles.css": "./dist/styles.css"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react && tailwindcss -i ./src/styles.css -o ./dist/styles.css",
    "dev": "tsup src/index.ts --format esm,cjs --dts --external react --watch & tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch",
    "clean": "rm -rf dist",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\"",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "classnames": "2.3.2",
    "utils": "*"
  },
  "devDependencies": {
    "@types/classnames": "2.3.1",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "eslint": "8.23.1",
    "eslint-config-custom": "*",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-config": "*",
    "tailwindcss": "3.1.8",
    "tsconfig": "*",
    "tsup": "6.2.3",
    "typescript": "4.8.3"
  }
}
