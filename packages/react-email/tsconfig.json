{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "tsconfig/react-library.json",
  "include": ["."],
  "exclude": ["tsconfig.export.json", "dist", "build", "node_modules"],
  "compilerOptions": {
    "target": "es5",
    "module": "CommonJS",
    "noUncheckedIndexedAccess": true,
    "resolveJsonModule": true,
    "outDir": "dist"
  }
}
